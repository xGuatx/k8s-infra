---
# Drupal Secrets - Generated from Ansible Vault
# This template is processed by Ansible to inject encrypted secrets

apiVersion: v1
kind: Secret
metadata:
  name: mysql-secret
  namespace: drupal
type: Opaque
stringData:
  mysql-root-password: "{{ mysql_root_password }}"
  mysql-password: "{{ mysql_password }}"
  mysql-database: "{{ mysql_database }}"
  mysql-user: "{{ mysql_user }}"
  mysql-replication-password: "{{ mysql_replication_password }}"
